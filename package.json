{
  "name": "@lstatro/cloud-search",
  "version": "1.7.0",
  "description": "cloud guardrail scanner",
  "main": "build/main.js",
  "types": "types.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.test.ts",
    "coverage": "nyc mocha -r ts-node/register src/**/*.test.ts",
    "reporter": "nyc --reporter=html mocha -r ts-node/register src/**/*.test.ts",
    "build": "rm -rf ./build && tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --check .",
    "preversion": "npm run build && npm run lint && npm run format && npm run test"
  },
  "readme": "https://github.com/lstatro/cloud-search/blob/develop/README.md",
  "bin": {
    "cloud-search": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lstatro/cloud-search.git"
  },
  "preferGlobal": true,
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "useTabs": false
  },
  "author": "lstatro",
  "license": "MIT",
  "keywords": [
    "security",
    "guardrail",
    "guardduty",
    "amazon",
    "aws",
    "ec2",
    "s3",
    "sqs",
    "sns",
    "rds",
    "iam",
    "ebs",
    "vpc"
  ],
  "bugs": {
    "url": "https://github.com/lstatro/cloud-search/issues"
  },
  "homepage": "https://github.com/lstatro/cloud-search#readme",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chalk": "^2.2.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.10",
    "@types/ora": "^3.2.0",
    "@types/sinon": "^9.0.8",
    "@types/yargs": "^15.0.9",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "aws-sdk-mock": "^5.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.11.0",
    "mocha": "^8.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "sinon": "^9.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "aws-sdk": "^2.773.0",
    "chalk": "^4.1.0",
    "ora": "^5.1.0",
    "yargs": "^16.1.0"
  }
}
